all: 	$(SLSRC)
	$(ARCH) $(ARCHFLAGS) $(SLICOTLIB) $(SLSRC)

$(SLSRC): $(FRC)

FRC:
	@FRC=$(FRC)

clean:
	rm -f *.o

.f.o: 
	$(FORTRAN) $(OPTS) -c $<
